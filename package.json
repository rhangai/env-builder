{
  "name": "@rhangai/env-builder",
  "repository": "git@github.com:renanhangai/env-builder.git",
  "author": "renanhangai <hangai.renan@gmail.com>",
  "version": "0.4.6",
  "description": "Create .env files for your projects with a single command",
  "main": "dist/index.js",
  "license": "MIT",
  "bin": {
    "env-builder": "dist/bin.js"
  },
  "dependencies": {
    "commander": "^4.1.1",
    "dependency-graph": "^0.8.1",
    "dotenv": "^8.2.0",
    "nanoid": "^2.1.11",
    "node-machine-id": "^1.1.12",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^13.7.0",
    "@types/uuid": "^8.3.4",
    "jest": "^27.4.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.1",
    "standard-version": "^7.1.0",
    "ts-jest": "^27.1.3",
    "typescript": "^3.7.5"
  },
  "scripts": {
    "build": "run-s 'build:clean' 'build:compile'",
    "build:clean": "rimraf dist",
    "build:compile": "tsc",
    "build:watch": "run-s 'build:clean' 'build:compile --watch'",
    "prepare": "run-s build",
    "release": "standard-version"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  }
}
